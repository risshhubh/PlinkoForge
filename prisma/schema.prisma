generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Round {
  id         Int      @id @default(autoincrement())
  serverSeed String
  clientSeed String
  nonce      Int
  pathJson   String
  result     Int
  
  createdAt  DateTime @default(now())
}
